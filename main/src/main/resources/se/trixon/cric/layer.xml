<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Actions">
        <folder name="Edit">
            <file name="org-openide-actions-DeleteAction.instance_hidden"/>
        </folder>
        <folder name="Help">
            <file name="org-netbeans-core-actions-AboutAction.instance">
                <attr name="delegate" newvalue="se.trixon.cric.actions.AboutAction"/>
                <attr name="instanceCreate" methodvalue="org.openide.awt.Actions.alwaysEnabled"/>
                <attr name="displayName" bundlevalue="org.netbeans.core.actions.Bundle#About"/>
                <attr name="noIconInMenu" boolvalue="true"/>
            </file>
        </folder>
    </folder>

    <folder name="Menu">
        <file name="Edit_hidden"/>
        <folder name="File">
            <file name="Separator2.instance_hidden"/>
            <file name="Separator3.instance_hidden"/>
            <file name="Separator4.instance_hidden"/>
            <file name="se-trixon-almond-nbp-actions-RestartAction.shadow_hidden"/>
            <file name="org-openide-actions-PageSetupAction.shadow_hidden"/>
            <file name="org-openide-actions-PrintAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAllAction.shadow_hidden"/>
            <file name="org-openide-actions-SaveAsAction.shadow_hidden"/>
        </folder>
        <file name="GoTo_hidden"/>
        <folder name="View_hidden"/>
        <folder name="Tools">
            <file name="org-netbeans-core-actions-LogAction.shadow">
                <attr name="originalFile" stringvalue="Actions/View/org-netbeans-core-actions-LogAction.instance"/>
            </file>
        </folder>
        <folder name="View">
            <file name="Separator2.instance_hidden"/>
            <file name="Separator3.instance_hidden"/>
            <file name="ShowEditorOnlyAction.shadow_hidden"/>
            <file name="ToggleFullScreenAction.shadow_hidden"/>
            <file name="org-netbeans-core-windows-actions-ToolbarsListAction.shadow_hidden"/>
        </folder>
        <file name="Window_hidden"/>
    </folder>

    <folder name="OptionsDialog">
        <folder name="Advanced">
            <file name="org-netbeans-core-ui-options-filetypes-FileAssociationsOptionsPanelController.instance_hidden"/>
        </folder>
        <folder name="Appearance">
            <file name="DocumentTabs.instance_hidden"/>
            <file name="Windows.instance_hidden"/>
        </folder>
        <file name="General.instance_hidden"/>
        <file name="Keymaps.instance_hidden"/>
    </folder>

    <folder name="Shortcuts">
        <file name="DS-ENTER.shadow_hidden"/>
        <file name="OS-ENTER.shadow_hidden"/>
        <file name="D-COMMA.shadow">
            <attr name="originalFile" stringvalue="Actions/Window/org-netbeans-modules-options-OptionsWindowAction.instance"/>
        </file>
    </folder>
    <file name="Toolbars_hidden"/>
</filesystem>
